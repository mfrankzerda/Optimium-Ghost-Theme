{{! Optimium - BLOG HEADER PARTIAL at Page }}

<header id="head_page_Optimium" data-speed="0.5" data-bleed="20">

    <div id="head_content_Optimium" class="boxed-cargo">

        {{#if @blog.logo}}
        <a href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="{{@blog.title}} Logo" /></a>
		{{else}}
        <h1 id="blog_title"><a href="{{@blog.url}}">{{@blog.title}}</a></h1>
		{{/if}}

        {{> menu}}

    </div>

</header>